<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jj.mall.admin.dao.UmsAdminRoleRelationDao">
    <select id="getUmsAdminRoleList" resultMap="com.jj.mall.mapper.UmsRoleMapper.BaseResultMap">
        select r.*
        from ums_admin_role_relation_dao arr, ums_role r
        where arr.role_id = r.id and arr.admin_id = #{adminId}
    </select>
</mapper>